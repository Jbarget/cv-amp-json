<!DOCTYPE html>
<html amp>

<head>
  <link rel="canonical" href="self.html" />
  <link rel="stylesheet" href="https://unpkg.com/tachyons@4.7.0/css/tachyons.min.css" />
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <style amp-boilerplate>
    body {
      -webkit-animation: -amp-start 8s steps(1, end) 0s 1 normal both;
      -moz-animation: -amp-start 8s steps(1, end) 0s 1 normal both;
      -ms-animation: -amp-start 8s steps(1, end) 0s 1 normal both;
      animation: -amp-start 8s steps(1, end) 0s 1 normal both
    }

    @-webkit-keyframes -amp-start {
      from {
        visibility: hidden
      }

      to {
        visibility: visible
      }
    }

    @-moz-keyframes -amp-start {
      from {
        visibility: hidden
      }

      to {
        visibility: visible
      }
    }

    @-ms-keyframes -amp-start {
      from {
        visibility: hidden
      }

      to {
        visibility: visible
      }
    }

    @-o-keyframes -amp-start {
      from {
        visibility: hidden
      }

      to {
        visibility: visible
      }
    }

    @keyframes -amp-start {
      from {
        visibility: hidden
      }

      to {
        visibility: visible
      }
    }
  </style><noscript>
    <style amp-boilerplate>
      body {
        -webkit-animation: none;
        -moz-animation: none;
        -ms-animation: none;
        animation: none
      }
    </style>
  </noscript>
  <style>
    html {
      -webkit-print-color-adjust: exact;
    }

    .min-vh-15 {
      min-height: 15vh;
    }

    .min-vh-6 {
      min-height: 6vh;
    }
  </style>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width,minimum-scale=1,initial-scale=1">
  <link rel="shortcut icon" href="./favicon.ico" type="image/x-icon">
  <link rel="icon" href="./favicon.ico" type="image/x-icon">
  <title>{{{title}}}</title>
</head>

<body class="helvetica fw3">
  <header class="tc bg-green pa2">
    <div class="inline-flex items-center">
      <div class="flex">
        <div class="w-40 flex justify-center">
          <div>
            <h1 class="f3 mb1 fw3">{{basic.first_name}} {{basic.last_name}} </h1>
            <p class="f6 fw3 mt0 mb1">{{basic.email}}</p>
          </div>
        </div>
        <p class="w-60 f5 mr3">{{{basic.intro}}}</p>
      </div>
    </div>
  </header>
  <main>
    <section>
      <ul class="list flex flex-wrap pl0 justify-center mv2">
        {{#each skills}}
        <li class="ba br-pill b--black-10 di pa2 mh1">{{this}}</li>
        {{/each}}
      </ul>
    </section>
    <div class="ba b--light-gray ma2"></div>
    <section class="ph3">
      <h2 class='ttu fw4 tc mb2'>Work</h2>
      <div class='flex flex-wrap justify-center'>
        {{#each work}}
        <div class="w-50 min-vh-15 tc mv1">
          <div>
            <p class="f4">{{{this.name}}} <span class="black-40 no-underline f6">{{this.from_to}}</span></p>
            <p class="black-40 no-underline">{{this.stack}}</p>
          </div>
          <p class="pv1 f6">{{this.details}}</p>
        </div>
        {{/each}}
      </div>
    </section>
    <section class="ph3">
      <h2 class='ttu fw4 tc'>Extra Things</h2>
      <ul class='flex justify-center flex-wrap'>
        {{#each extra_curricular}}
        <li class="w-50 min-vh-6 pr4">{{this}}</li>
        {{/each}}
      </ul>
    </section>
    <section class="ph3">
      <h2 class='ttu fw4 tc'>Education</h2>
      {{#each education}}
      <div class="tc">
        <p>{{this.name}}</p>
        <p>{{this.details}}</p>
      </div>
      {{/each}}
    </section>
  </main>
  <script async src="https://cdn.ampproject.org/v0.js"></script>
</body>

</html>